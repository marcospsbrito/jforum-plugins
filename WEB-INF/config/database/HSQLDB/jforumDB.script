CREATE TABLE JFORUM_CONFIG(CONFIG_NAME VARCHAR(255) DEFAULT '' NOT NULL,CONFIG_VALUE VARCHAR(255) DEFAULT '' NOT NULL,CONFIG_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY)

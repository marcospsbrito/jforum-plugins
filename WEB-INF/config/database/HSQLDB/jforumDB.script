CREATE TABLE JFORUM_BANLIST(BANLIST_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,USER_ID INTEGER DEFAULT '0' NOT NULL,BANLIST_IP VARCHAR(8) DEFAULT '' NOT NULL,BANLIST_EMAIL VARCHAR(255) DEFAULT NULL)
CREATE INDEX IDX_BANLIST_USER ON JFORUM_BANLIST(USER_ID)

CREATE INDEX IDX_UG_USER ON JFORUM_USER_GROUPS(USER_ID)
CREATE TABLE JFORUM_ROLES(ROLE_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,GROUP_ID INTEGER DEFAULT '0',USER_ID INTEGER DEFAULT '0',NAME VARCHAR(255) NOT NULL,TYPE INTEGER DEFAULT 1)
CREATE INDEX IDX_ROLES_GROUP ON JFORUM_ROLES(GROUP_ID)
CREATE INDEX IDX_ROLES_USER ON JFORUM_ROLES(USER_ID)
CREATE INDEX IDX_ROLES_NAME ON JFORUM_ROLES(NAME)
CREATE TABLE JFORUM_ROLE_VALUES(ROLE_ID INTEGER NOT NULL,VALUE VARCHAR(255),TYPE INTEGER DEFAULT 1)
CREATE INDEX IDX_RV_ROLE ON JFORUM_ROLE_VALUES(ROLE_ID)
